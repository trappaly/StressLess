{
  "name": "stressless",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "backend:build": "pnpm --filter backend build",
    "frontend:build": "pnpm --filter frontend build",
    "build:all": "concurrently -c \"auto\" \"pnpm backend:build\" \"pnpm frontend:build\"",
    "backend:start": "pnpm --filter backend start",
    "frontend:start": "pnpm --filter frontend start",
    "start:all": "concurrently -c \"auto\" \"pnpm backend:start\" \"pnpm frontend:start\"",
    "backend:test:unit": "pnpm --filter backend test",
    "frontend:test:unit": "pnpm --filter frontend test",
    "test:unit": "concurrently -c \"auto\" \"pnpm backend:test:unit\" \"pnpm frontend:test:unit\"",
    "lint": "pnpm exec eslint frontend backend --ext .ts,.tsx --fix --ignore-pattern 'frontend/.next/*' --ignore-pattern 'backend/dist/*'",
    "format": "pnpm exec prettier --write frontend/**/*.{ts,tsx} backend/**/*.ts",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",

    "@types/express": "^5.0.1",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bcrypt": "^5.1.1",

    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "firebase": "^11.6.0",

    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",

    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^2.1.0",

    "pg": "^8.14.1",
    "concurrently": "^9.1.2"

    "pg": "^8.14.1"

  }
}
